<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane HBox.hgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <VBox alignment="CENTER_LEFT" minHeight="25" GridPane.columnIndex="0">
      <padding>
        <Insets top="5" right="5" bottom="5" left="5" />
      </padding>
      <HBox spacing="5" alignment="CENTER_LEFT">
        <Label fx:id="elderlyName" text="\$first" styleClass="cell_big_label" />
        <Label styleClass="cell_small_label" text="paired with volunteer:" />
        <Label fx:id="volunteerName" text="\$first" styleClass="cell_big_label" />
      </HBox>
    </VBox>
  </GridPane>
</HBox>
